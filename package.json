{
  "name": "@orangejellyfish/serverless-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "deploy": "export NODE_ENV=dev && npm run deploy:env",
    "deploy:env": "sls deploy --stage $NODE_ENV",
    "deploy:production": "export NODE_ENV=production && npm run deploy:env",
    "package": "sls package",
    "lint": "eslint ./src",
    "start": "serverless offline start",
    "test": "jest --watch src",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "engines": {
    "node": "14.17.0"
  },
  "dependencies": {
    "aws-sdk": "2.880.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^27.0.2",
    "babel-loader": "^8.2.2",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "husky": "^6.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "serverless": "2.47.0",
    "serverless-iam-roles-per-function": "^3.1.0",
    "serverless-offline": "^7.0.0",
    "serverless-plugin-split-stacks": "^1.11.0",
    "serverless-webpack": "^5.5.1",
    "webpack": "^5.39.1"
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint -- --fix"
    ]
  },
  "jest": {
    "collectCoverage": true
  }
}
