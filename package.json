{
  "name": "@orangejellyfish/serverless-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "deploy": "export NODE_ENV=dev && npm run deploy:env",
    "deploy:env": "sls deploy --stage $NODE_ENV",
    "deploy:production": "export NODE_ENV=production && npm run deploy:env",
    "package": "sls package",
    "lint": "eslint ./src",
    "start": "serverless offline start",
    "test": "jest --watch src",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "engines": {
    "node": "14.15.4"
  },
  "dependencies": {
    "aws-sdk": "2.804.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^5.2.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "serverless": "2.31.0",
    "serverless-iam-roles-per-function": "^3.1.0",
    "serverless-offline": "^6.9.0",
    "serverless-plugin-split-stacks": "^1.11.0",
    "serverless-webpack": "^5.4.0",
    "webpack": "^5.28.0"
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint -- --fix"
    ]
  },
  "jest": {
    "collectCoverage": true
  }
}
